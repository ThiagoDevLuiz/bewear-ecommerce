{
  "eslint.workingDirectories": [
    {
      "node": "auto"
    }
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "cSpell.words": ["BEWEAR", "ECOMMERCE", "sonner"]
}
